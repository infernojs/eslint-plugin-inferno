{
  "name": "eslint-plugin-inferno",
  "version": "7.29.3",
  "description": "Inferno specific linting rules for ESLint",
  "main": "index.js",
  "scripts": {
    "debug": "node --inspect-brk  node_modules/mocha/bin/_mocha tests/lib/**/*.js tests/util/**/*.js tests/index.js",
    "lint": "eslint ./",
    "postlint": "npm run type-check",
    "pretest": "npm run lint",
    "test": "npm run unit-test",
    "posttest": "aud --production",
    "type-check": "tsc",
    "unit-test": "istanbul cover node_modules/mocha/bin/_mocha tests/lib/**/*.js tests/util/**/*.js tests/index.js",
    "generate-list-of-rules": "md-magic --path README.md",
    "generate-list-of-rules:check": "npm run generate-list-of-rules && git diff --exit-code README.md"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/infernojs/eslint-plugin-inferno"
  },
  "homepage": "https://github.com/infernojs/eslint-plugin-inferno",
  "bugs": "https://github.com/infernojs/eslint-plugin-inferno/issues",
  "dependencies": {
    "doctrine": "^3.0.0",
    "estraverse": "^5.3.0",
    "jsx-ast-utils": "^3.2.1",
    "minimatch": "^5.0.1",
    "object.entries": "^1.1.5",
    "object.fromentries": "^2.0.5",
    "object.hasown": "^1.1.0",
    "object.values": "^1.1.5",
    "prop-types": "^15.8.1",
    "resolve": "^2.0.0-next.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-decorators": "^7.17.0",
    "@babel/plugin-syntax-do-expressions": "^7.16.7",
    "@babel/plugin-syntax-function-bind": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@types/eslint": "=8.4.1",
    "@types/estree": "^0.0.51",
    "@types/node": "^17.0.21",
    "@typescript-eslint/parser": "^5.13.0",
    "array.prototype.flatmap": "^1.2.5",
    "aud": "^2.0.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-inferno": "^6.3.0",
    "eslint": "^8",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-import": "^2.25.4",
    "espree": "^9.3.1",
    "istanbul": "^0.4.5",
    "markdown-magic": "^2.6.0",
    "mocha": "^9.2.1",
    "semver": "^7.3.5",
    "sinon": "^13.0.1",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "eslint": "^6 || ^7 || ^8"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "inferno"
  ],
  "license": "MIT",
  "greenkeeper": {
    "ignore": [
      "semver"
    ]
  }
}
