{
  "name": "eslint-plugin-inferno",
  "version": "7.32.2",
  "description": "Inferno specific linting rules for ESLint",
  "main": "index.js",
  "scripts": {
    "prepack": "npmignore --auto --commentLines=autogenerated",
    "debug": "node --inspect-brk  node_modules/mocha/bin/_mocha tests/lib/**/*.js tests/util/**/*.js tests/index.js",
    "postlint:docs": "npm run update:eslint-docs -- --check",
    "lint": "eslint .",
    "postlint": "npm run type-check",
    "pretest": "npm run lint",
    "test": "npm run unit-test",
    "posttest": "aud --production",
    "type-check": "tsc",
    "unit-test": "istanbul cover node_modules/mocha/bin/_mocha tests/lib/**/*.js tests/util/**/*.js tests/index.js",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/infernojs/eslint-plugin-inferno"
  },
  "homepage": "https://github.com/infernojs/eslint-plugin-inferno",
  "bugs": "https://github.com/infernojs/eslint-plugin-inferno/issues",
  "dependencies": {
    "doctrine": "^3.0.0",
    "array.prototype.tosorted": "^1.1.1",
    "estraverse": "^5.3.0",
    "jsx-ast-utils": "^3.3.3",
    "minimatch": "^8.0.3",
    "object.entries": "^1.1.6",
    "object.fromentries": "^2.0.6",
    "object.hasown": "^1.1.2",
    "object.values": "^1.1.6",
    "prop-types": "^15.8.1",
    "resolve": "^2.0.0-next.4",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-syntax-decorators": "^7.21.0",
    "@babel/plugin-syntax-do-expressions": "^7.18.6",
    "@babel/plugin-syntax-function-bind": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@types/eslint": "=8.37.0",
    "@types/estree": "^1.0.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/parser": "^5.57.1",
    "aud": "^2.0.2",
    "babel-eslint": "^8.2.6",
    "babel-plugin-inferno": "^6.6.0",
    "eslint": "^8",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-doc-generator": "^1.4.3",
    "eslint-plugin-eslint-plugin": "^5.0.8",
    "eslint-plugin-import": "^2.27.5",
    "espree": "^9.5.1",
    "istanbul": "^0.4.5",
    "markdownlint-cli": "^0.33.0",
    "mocha": "^10.2.0",
    "npmignore": "^0.3.0",
    "sinon": "^15.0.3",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "eslint": "^6 || ^7 || ^8"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "inferno"
  ],
  "license": "MIT",
  "publishConfig": {
    "ignore": [
      ".github/",
      "!lib",
      "docs/",
      "test/",
      "tests/",
      "*.md",
      "*.config.js",
      ".eslint-doc-generatorrc.js",
      ".eslintrc",
      ".editorconfig",
      "tsconfig.json",
      ".markdownlint*"
    ]
  }
}
