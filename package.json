{
  "name": "eslint-plugin-inferno",
  "version": "7.14.3",
  "description": "Inferno specific linting rules for ESLint",
  "main": "index.js",
  "scripts": {
    "coveralls": "cat ./reports/coverage/lcov.info | coveralls",
    "lint": "eslint ./",
    "postlint": "npm run type-check",
    "pretest": "npm run lint",
    "test": "npm run unit-test",
    "type-check": "tsc",
    "unit-test": "istanbul cover --dir reports/coverage node_modules/mocha/bin/_mocha tests/lib/**/*.js tests/util/**/*.js tests/index.js",
    "debug-test": "node --inspect-brk node_modules/mocha/bin/_mocha tests/lib/**/*.js tests/util/**/*.js tests/index.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/infernojs/eslint-plugin-inferno"
  },
  "homepage": "https://github.com/infernojs/eslint-plugin-inferno",
  "bugs": "https://github.com/infernojs/eslint-plugin-inferno/issues",
  "dependencies": {
    "array-includes": "^3.0.3",
    "doctrine": "^3.0.0",
    "has": "^1.0.3",
    "jsx-ast-utils": "^2.2.1",
    "object.entries": "^1.1.0",
    "object.fromentries": "^2.0.0",
    "object.values": "^1.1.0",
    "resolve": "^1.12.0"
  },
  "devDependencies": {
    "@types/eslint": "^4.16.8",
    "@types/estree": "0.0.39",
    "@types/node": "^12.7.1",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.2",
    "coveralls": "^3.0.6",
    "eslint": "^3.0.0 || ^4.0.0 || ^5.0.0 || ^6.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "sinon": "^7.4.1",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "eslint": "^3.0.0 || ^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "inferno"
  ],
  "license": "MIT"
}
