{
  "name": "eslint-plugin-inferno",
  "version": "6.10.2",
  "description": "Inferno specific linting rules for ESLint",
  "main": "index.js",
  "scripts": {
    "coveralls": "cat ./reports/coverage/lcov.info | coveralls",
    "lint": "eslint ./",
    "test": "npm run lint && npm run unit-test",
    "unit-test": "istanbul cover --dir reports/coverage node_modules/mocha/bin/_mocha tests/**/*.js -- --reporter dot --opts tests/mocha.opts"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/infernojs/eslint-plugin-inferno"
  },
  "homepage": "https://github.com/infernojs/eslint-plugin-inferno",
  "bugs": "https://github.com/infernojs/eslint-plugin-inferno",
  "dependencies": {
    "doctrine": "^2.0.0",
    "has": "^1.0.1",
    "jsx-ast-utils": "^1.4.0",
    "array.prototype.find": "^2.0.3",
    "object.assign": "^4.0.4"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "coveralls": "2.11.16",
    "eslint": "^3.16.1",
    "istanbul": "0.4.5",
    "mocha": "3.2.0"
  },
  "peerDependencies": {
    "eslint": "^3.16.1"
  },
  "engines": {
    "node": ">=0.10"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "inferno"
  ],
  "license": "MIT"
}
