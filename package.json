{
  "name": "eslint-plugin-inferno",
  "version": "7.37.4",
  "description": "Inferno specific linting rules for ESLint",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "clean-built-types": "rm -f $(find . -maxdepth 1 -type f -name '*.d.ts*') $(find lib -type f -name '*.d.ts*' ! -name 'types.d.ts')",
    "debug": "node --inspect-brk  node_modules/mocha/bin/_mocha tests/lib/**/*.js tests/util/**/*.js tests/index.js",
    "prebuild-types": "npm run clean-built-types",
    "build-types": "tsc -p build.tsconfig.json",
    "prepack": "npm run build-types && npmignore --auto --commentLines=autogenerated",
    "postlint:docs": "npm run update:eslint-docs -- --check",
    "lint": "eslint .",
    "postlint": "npm run type-check",
    "pretest": "npm run lint",
    "test": "npm run unit-test",
    "posttest": "npx npm@'>= 10.2' audit --production",
    "type-check": "tsc",
    "unit-test": "mocha tests/lib/**/*.js tests/util/**/*.js tests/index.js tests/flat-config.js",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/infernojs/eslint-plugin-inferno"
  },
  "homepage": "https://github.com/infernojs/eslint-plugin-inferno",
  "bugs": "https://github.com/infernojs/eslint-plugin-inferno/issues",
  "directories": {
    "test": [
      "test",
      "tests",
      "test-published-types"
    ]
  },
  "dependencies": {
    "doctrine": "^3.0.0",
    "es-iterator-helpers": "^1.2.1",
    "estraverse": "^5.3.0",
    "jsx-ast-utils": "^3.3.5",
    "minimatch": "^10.0.1",
    "resolve": "^2.0.0-next.5",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/plugin-syntax-decorators": "^7.25.9",
    "@babel/plugin-syntax-do-expressions": "^7.25.9",
    "@babel/plugin-syntax-function-bind": "^7.25.9",
    "@types/eslint": "=9.6.1",
    "@types/estree": "^1.0.6",
    "@types/node": "^22.10.7",
    "@typescript-eslint/parser": "^7 || ^8",
    "babel-eslint": "^9 || ^10",
    "babel-plugin-inferno": "^6.7.2",
    "eslint":  "^9",
    "eslint-doc-generator": "^2.0.2",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "eslint-plugin-import": "^2.31.0",
    "espree": "^10.3.0",
    "jackspeak": "=4.0.2",
    "markdownlint-cli": "^0.43.0",
    "mocha": "^11.1.0",
    "glob": "=11.0.1",
    "npmignore": "^0.3.1",
    "sinon": "^19.0.2",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "eslint": "^8 || ^9.7"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "inferno"
  ],
  "license": "MIT",
  "publishConfig": {
    "ignore": [
      ".github/",
      "!lib",
      "docs/",
      "test/",
      "test-published-types/",
      "tests/",
      "*.md",
      "*.config.js",
      ".eslint-doc-generatorrc.js",
      ".eslintrc",
      ".editorconfig",
      "tsconfig.json",
      "build.tsconfig.json",
      ".markdownlint*",
      "types",
      "!*.d.ts",
      "!*.d.ts.map"
    ]
  }
}
