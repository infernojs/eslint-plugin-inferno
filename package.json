{
  "name": "eslint-plugin-inferno",
  "version": "7.33.2",
  "description": "Inferno specific linting rules for ESLint",
  "main": "index.js",
  "scripts": {
    "prepack": "npmignore --auto --commentLines=autogenerated",
    "debug": "node --inspect-brk  node_modules/mocha/bin/_mocha tests/lib/**/*.js tests/util/**/*.js tests/index.js",
    "postlint:docs": "npm run update:eslint-docs -- --check",
    "lint": "eslint .",
    "postlint": "npm run type-check",
    "pretest": "npm run lint",
    "test": "npm run unit-test",
    "posttest": "aud --production",
    "type-check": "tsc",
    "unit-test": "mocha tests/lib/**/*.js tests/util/**/*.js tests/index.js",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/infernojs/eslint-plugin-inferno"
  },
  "homepage": "https://github.com/infernojs/eslint-plugin-inferno",
  "bugs": "https://github.com/infernojs/eslint-plugin-inferno/issues",
  "dependencies": {
    "array.prototype.tosorted": "^1.1.2",
    "doctrine": "^3.0.0",
    "es-iterator-helpers": "^1.0.15",
    "estraverse": "^5.3.0",
    "jsx-ast-utils": "^3.3.5",
    "minimatch": "^9.0.3",
    "resolve": "^2.0.0-next.5",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-syntax-decorators": "^7.23.3",
    "@babel/plugin-syntax-do-expressions": "^7.23.3",
    "@babel/plugin-syntax-function-bind": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@types/eslint": "=8.44.8",
    "@types/estree": "^1.0.5",
    "@types/node": "^20.10.4",
    "@typescript-eslint/parser": "^5.62.0",
    "aud": "^2.0.4",
    "babel-eslint": "^8.2.6",
    "babel-plugin-inferno": "^6.7.0",
    "eslint": "^8",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-doc-generator": "^1.6.1",
    "eslint-plugin-eslint-plugin": "^5.2.1",
    "eslint-plugin-import": "^2.29.0",
    "espree": "^9.6.1",
    "jackspeak": "=2.3.6",
    "markdownlint-cli": "^0.38.0",
    "mocha": "^10.2.0",
    "glob": "=10.3.10",
    "npmignore": "^0.3.1",
    "sinon": "^17.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "eslint": "^6 || ^7 || ^8"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "inferno"
  ],
  "license": "MIT",
  "publishConfig": {
    "ignore": [
      ".github/",
      "!lib",
      "docs/",
      "test/",
      "tests/",
      "*.md",
      "*.config.js",
      ".eslint-doc-generatorrc.js",
      ".eslintrc",
      ".editorconfig",
      "tsconfig.json",
      ".markdownlint*"
    ]
  }
}
